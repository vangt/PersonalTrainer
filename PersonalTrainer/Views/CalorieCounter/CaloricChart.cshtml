
@model PersonalTrainer.ViewModels.ChartViewModel
@{
    Layout = null;
}

@{
    var myChart = new Chart(width:600, height:400)
    .AddTitle("Caloric Intake")
    .AddSeries(
    name: "Calorie Intake",
    chartType: "line",
    xValue: Model.DateAdded,
    yValues: Model.Calories)
    .Write();
}